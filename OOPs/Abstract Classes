abstract class Animal{
    abstract void makeSound();
    void sleep(){
        System.out.println("Sleep");
    }
}
class Dog extends Animal{
    @Override
    void makeSound() {
        System.out.println("Woof");
    }
}

public class Main {
    public static void main(String[] args) {
        Dog d = new Dog();
        d.makeSound();
        d.sleep();
    }
}
