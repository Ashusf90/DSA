class Student{
    String name;
    int age;
//    Default constructor
    Student(){
        name = "Not Assigned";
        age = 0;
    }
    void display(){
        System.out.println(name + " is " + age + "years old");
    }
}
public class Main {
    public static void main(String[] args) {
        Student s1 = new Student(); //Default constructor is called
        s1.display();
    }
}
