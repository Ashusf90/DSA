public class Main {
    public static void main(String[] args) {
//    object
        Student s1 = new Student();
        s1.name = "Ashu";
        s1.age  = 20;
        s1.display();

    }
//    class
    static class Student{
        String name;
        int age;
        void display(){
            System.out.println(name + " is " + age);
        }
    }
}
