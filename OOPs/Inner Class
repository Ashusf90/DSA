
public class Main {
    static class Test{
        String name;
        public Test(String name){
            this.name = name;
        }
    }

    public static void main(String[] args) {
        Test a =new Test("Ashu");
        Test b = new Test("Khushi");
        System.out.println(a.name);
        System.out.println(b.name);
    }
}
