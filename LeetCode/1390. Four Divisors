class Solution {
    public int sumFourDivisors(int[] nums) {
        int res=0;
        for(int num:nums){
            int count = 0;
        int sum=0;
            for(int i = 1; i<=num;i++){
                if(num%i==0){
                    count++;
                    sum=sum+i;
                }
                if(count>4){
                    break;
                }
                
            }
            if(count==4){
                    res+=sum;
                }
            
        }
        return res;
        
    }
}

//Bad Time Complexity
