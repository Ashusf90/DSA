class Solution {
    public List<String> buildArray(int[] target, int n) {
        List<String> result = new ArrayList<>();
        int index=0;
        int l = target.length;
        for(int i = 1; i<=n&&index<l; i++){
            
            result.add("Push");
            if(i == target[index]){
                index++;
            }else{
                result.add("Pop");
            }
        }
        return result;
    }
}
