class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        int count = 0;
        for (int a:apple) {
            count+=a;
        }
        Arrays.sort(capacity);
        int boxes=0;
        for (int i=capacity.length-1;i>=0&&count>0;i--) {
            count -= capacity[i];
            boxes++;
        }
        if(count<=0){
            return boxes;
        }else{
            return -1;
        }
        
    }
}
