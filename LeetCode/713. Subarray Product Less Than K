class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
        int ans = 0;
        int curr = 0;
        long product =1;
        if(k<=1){
return 0;
        } 
        for(int i = 0; i<nums.length;i++){
            product*=nums[i];
            while(product>=k){
                product /= nums[curr];
                curr++;
            }
            ans += (i - curr + 1);
        }
        return ans;
    }
}

