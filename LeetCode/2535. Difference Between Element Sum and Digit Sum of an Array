class Solution {
    public int differenceOfSum(int[] nums) {
        int sum=0;
        int totalsum=0;
        for(int num:nums){
            while(num!=0){
                totalsum+=num%10;
                num/=10;
            }
            
        }
        for(int i = 0;i<nums.length;i++){
             sum=sum+nums[i];
        }
        return sum-totalsum;
    }
}
